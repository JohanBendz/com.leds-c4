{
  "id": "com.leds-c4",
  "sdk": 1,
  "name": {
    "en": "LEDS-C4"
  },
  "description": {
    "en": "Homey support for LEDS-C4 devices."
  },
  "images": {
    "large": "./assets/images/large.jpg",
    "small": "./assets/images/small.jpg"
  },
  "category": "climate",
  "version": "1.0.0",
  "compatibility": ">=1.3",
  "author": {
    "name": "Erik Scheffers",
    "email": "erik@scheffers.org"
  },
  "signals": {
    "433": {
      "pt2260": {
        "sof": [],
        "eof": [
          239
        ],
        "words": [
          [
            239,
            759,
            238,
            759
          ],
          [
            759,
            239,
            759,
            239
          ],
          [
            239,
            759,
            759,
            239
          ]
        ],
        "interval": 2500,
        "repetitions": 5,
        "sensitivity": 0.5,
        "minimalLength": 12,
        "maximalLength": 12
      }
    }
  },
  "drivers": [
    {
      "id": "ceiling_fan",
      "name": {
        "en": "Ceiling Fan",
        "nl": "Plafondventilator"
      },
      "class": "fan",
      "capabilities": [
        "onoff",
        "dim"
      ],
      "images": {
        "small": "./433_generator/assets/ceiling_fan/images/small.jpg",
        "large": "./433_generator/assets/ceiling_fan/images/large.jpg"
      },
      "pair": [
        {
          "id": "generic_choice",
          "navigation": {}
        },
        {
          "id": "generic_imitate",
          "navigation": {
            "prev": "generic_choice"
          }
        },
        {
          "id": "generic_test_switch_2",
          "navigation": {
            "prev": "generic_imitate",
            "next": "generic_done"
          }
        },
        {
          "id": "generic_info",
          "navigation": {
            "prev": "generic_choice",
            "next": "generic_dipswitch"
          }
        },
        {
          "id": "generic_dipswitch",
          "navigation": {
            "prev": "generic_info",
            "next": "generic_test_switch"
          }
        },
        {
          "id": "generic_test_switch",
          "navigation": {
            "prev": "generic_dipswitch",
            "next": "generic_done"
          }
        },
        {
          "id": "generic_done",
          "navigation": {}
        }
      ]
    },
    {
      "id": "fan_remote",
      "name": {
        "en": "Remote",
        "nl": "Afstandsbediening"
      },
      "class": "other",
      "capabilities": [],
      "images": {
        "small": "./433_generator/assets/fan_remote/images/small.jpg",
        "large": "./433_generator/assets/fan_remote/images/large.jpg"
      },
      "pair": [
        {
          "id": "generic_imitate",
          "navigation": {}
        },
        {
          "id": "generic_test_remote",
          "navigation": {
            "prev": "generic_imitate",
            "next": "generic_done"
          }
        },
        {
          "id": "generic_done",
          "navigation": {}
        }
      ]
    }
  ],
